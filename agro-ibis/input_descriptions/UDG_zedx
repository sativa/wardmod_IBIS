correct_sphumid = .FALSE.
conserve_tdew = .TRUE.

istyrd = 1948
nanomd = 60

level_var = 'lev'

fn_rads_mon_clim = 'zedx/Intr_RADS_60year_climo'
fn_rads_daily = 'zedx/daily/Intr_rads'
fn_tmin_mon_clim = 'zedx/Intr_TMIN_60year_climo'
fn_tmin_daily = 'zedx/daily/Intr_tmin'
fn_tmax_mon_clim = 'zedx/Intr_TMAX_60year_climo'
fn_tmax_daily = 'zedx/daily/Intr_tmax'
fn_prec_mon_clim = 'zedx/Intr_PREC_60year_climo_mm_day'
fn_prec_daily = 'zedx/daily/Intr_prec'
