fn_wspd_daily = 'zedx/daily/Intr_wspd'
fn_wetd_mon_clim = ' '  ! no wetd variable specified for this data set
fn_wetd_mon = ' '       ! no wetd variable specified for this data set
fn_tminavgann = 'zedx/Intr_TMIN_annual_60year_climo'

! Just specify variable names that differ from their default values:
var_rh_mon_clim = 'relh'
var_rh_daily = 'relh'
var_tminavgann = 'tmin'

nc_file_bufrsize_KB = 8

